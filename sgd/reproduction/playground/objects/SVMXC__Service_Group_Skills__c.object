    <validationRules>
        <fullName>SMAX_PS_Check_Skill_Level</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
 OR( 
   SVMXC__Skill_Level__c &lt;  SVMXC__Skill__r.SVMXC__Skill_Level_Low__c,
   SVMXC__Skill_Level__c &gt;   SVMXC__Skill__r.SVMXC__Skill_Level_High__c 
), true, false)</errorConditionFormula>
        <errorDisplayField>SVMXC__Skill_Level__c</errorDisplayField>
        <errorMessage>技能レベルをスキル・資格での範囲内に設定ください。</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SVMXC__Expertise_Start_End_Date</fullName>
