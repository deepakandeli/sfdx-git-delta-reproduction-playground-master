    <webLinks>
        <fullName>mkto_si__Add_to_Watch_List</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Add to Watch List</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/apex.js&quot;)} 

var idArr = {!GETRECORDIDS($ObjectType.Account)};
if (idArr[0] == null){
	alert(&apos;Please select at least one row&apos;);
} else {
	var value = new Array();
	value[0] = new String(idArr);
	var name = new Array();
	name[0] = &quot;contactIds&quot;;  //This is the default for all id arrays in controller, though confusing	
	try {
		var key = sforce.apex.execute(&quot;mkto_si.LongGetMethodArguHandler&quot;, &quot;putArgus&quot;, { name:name, value:value, contactType:&quot;Account&quot; }); 
		window.top.location = &quot;/apex/mkto_si__Add_to_Watch_List?contactType=Account&amp;key=&quot; + key +&quot;&amp;retUrl=&quot; + encodeURIComponent(document.location.href);	
	} 
	catch(err) {
		window.top.location = &quot;/apex/mkto_si__Add_to_Watch_List?contactType=Account&amp;contactIds=&quot; + idArr +&quot;&amp;retUrl=&quot; + encodeURIComponent(document.location.href);
	}
}</url>
    </webLinks>
