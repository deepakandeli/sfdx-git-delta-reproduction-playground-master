    <fields>
        <fullName>SMAX_PS_Expense_Percent__c</fullName>
        <externalId>false</externalId>
        <formula>IF(ISPICKVAL(SMAX_PS_Service_Quote__r.SMAX_PS_Expense_Percent__c, &apos;10&apos;), 10,IF(ISPICKVAL(SMAX_PS_Service_Quote__r.SMAX_PS_Expense_Percent__c, &apos;15&apos;), 15,20 )  )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>諸経費パーセント</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Percent</type>
    </fields>
    <fields>
        <fullName>SMAX_PS_Expense_Total_Final__c</fullName>
        <externalId>false</externalId>
        <formula>(CEILING((SMAX_PS_Other_Sub_Total__c + 
	          SMAX_PS_Labor_Sub_Total__c +
	          SMAX_PS_Parts_Sub_Total__c +
	          SMAX_PS_Equip_Sub_Total__c +
	          SMAX_PS_Expense_Total_Temp__c )*0.001) * 1000 ) - (SMAX_PS_Other_Sub_Total__c + 
	                                                           SMAX_PS_Labor_Sub_Total__c +
	                                                           SMAX_PS_Parts_Sub_Total__c +
	                                                           SMAX_PS_Equip_Sub_Total__c)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>諸経費</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>SMAX_PS_Expense_Total_Temp__c</fullName>
        <externalId>false</externalId>
        <formula>(SMAX_PS_Parts_Exp_Sub_Total__c + 
	SMAX_PS_Labor_Exp_Sub_Total__c + 
	SMAX_PS_Equip_Exp_Sub_Total__c + 
	SMAX_PS_Other_Exp_Sub_Total__c) * (SMAX_PS_Expense_Percent__c/100)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>諸経費（仮）</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
